# Copy to engine/.env and fill with your values. Do not commit real secrets.

# Database (Postgres via Supabase)
SUPABASE_DB_URL=postgresql://user:pass@host:5432/dbname?sslmode=require
DB_BACKEND=postgres

# API CORS
PROD_ORIGIN=https://your-production-domain

# Discord webhook for notifications (webhook_handler)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/xxx/yyy

